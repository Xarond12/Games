_G.ToggleColor = Color3.fromRGB(255,0,0)
_G.ButtonColor = Color3.fromRGB(0,255,0)
_G.SliderColor = Color3.fromRGB(0,0,255)

local library = loadstring(game:HttpGet(('https://pastebin.com/raw/FsJak6AT')))() -- It's obfuscated, I won't let you see my ugly coding skills. =)

local w = library:CreateWindow("Mega Noob Simulator")

local b = w:CreateFolder("Auto Buy")
local e = w:CreateFolder("Auto Kill")
local f = w:CreateFolder("Miscellaneous")

f:Label("eprx#9921",Color3.fromRGB(0,0,0),Color3.fromRGB(255,255,255))

e:Toggle("Auto Kill",function(bool)
    killi = bool
end)

e:Toggle("Boss Farm",function(bool)
    bosss = bool
end)

b:Toggle("Powerups",function(bool)
    shared.toggle = bool
end)

b:Toggle("Weights",function(boool)
    weightsss = boool
end)

b:Toggle("Pets",function(boool)
    pett = boool
end)

spawn(function()
while wait() do
if bosss then
wait()
game:GetService("ReplicatedStorage").Remotes.BossFight:FireServer("Mega Bacon")
wait(0.5)
for i = 1,25 do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace["Mega Bacon"].HumanoidRootPart.CFrame + Vector3.new(0,10,0)
game:GetService("ReplicatedStorage").Remotes.Punch:FireServer(workspace["Mega Bacon"])
end
end
end
end)


spawn(function()
while wait() do
if killi then
wait()
for i,v in pairs(game:GetService("Workspace").BaconStuff:GetDescendants()) do
if v.Name == "Humanoid" then
if v.Health > 0 then
for i = 1,10 do
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v.Parent:WaitForChild("HumanoidRootPart").CFrame + Vector3.new(0,5,0)
game:GetService("ReplicatedStorage").Remotes.Punch:FireServer(v.Parent)
end
end
end
end
end
end
end)

spawn(function()
while wait() do
if convert then
wait()
local ExchangeHeads = game:GetService("ReplicatedStorage").Remotes.ExchangeHeads
ExchangeHeads:FireServer(
	tonumber(game:GetService("Players").LocalPlayer.PlayerGui.CurrencyFrames.Noobs.Stats.Stat.Text)
)
end
end
end)

spawn(function()
while wait() do
if weightsss then
wait()
-- Auto Buy

local weights = {}

for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.ShopFrame.Menu.Upgrades:GetChildren()) do
    if v:IsA("Frame") then
        table.insert(weights, v.Name)
    end
    end

for i2,v2 in pairs(weights) do
wait(0.1)
local Buy = game:GetService("ReplicatedStorage").Remotes.Buy
Buy:FireServer(
	v2,
	"Damage"
)
end
end
end
end)

spawn(function()
while wait() do
if shared.toggle then
wait()
local pw = {}

for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.ShopFrame.Menu.Items.Powerup:GetChildren()) do
    if v:IsA("Frame") then
        table.insert(pw, v.Name)
    end
    end

for i2,v2 in pairs(pw) do
wait(0.1)
local Buy = game:GetService("ReplicatedStorage").Remotes.Buy
Buy:FireServer(
	v2
)
end
end
end
end)

f:Toggle("Heads to Coins",function(bool)
    convert = bool
end)

f:Toggle("TP Coins",function(bool)
    coins = bool
end)

-- This script was generated by hush's RemoteSpy

local Buy = game:GetService("ReplicatedStorage").Remotes.Buy
Buy:FireServer(
	"Muscle Head"
)

spawn(function()
while wait() do
if pett then
wait()
local pet = {}

for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.ShopFrame.Menu.Items.Pet:GetChildren()) do
    if v:IsA("Frame") then
        table.insert(pet, v.Name)
    end
    end

for i2,v2 in pairs(pet) do
wait(0.1)
local Buy = game:GetService("ReplicatedStorage").Remotes.Buy
Buy:FireServer(
	v2
)
end
end
end
end)

spawn(function()
    while wait() do
        if coins then
            for i,v in pairs(game:GetService("Workspace").CoinStuff:GetDescendants()) do
                if v.Name == "CoinCollectible" then
                    v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                end
            end
        end
    end
    end)

e:Button("Hardest Stage",function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1294.13928, 1315.99243, -2083.01636, 0.999615014, 1.04860831e-08, -0.0277466774, -1.08416893e-08, 1, -1.26657094e-08, 0.0277466774, 1.2961654e-08, 0.999615014)
end)

f:Slider("WalkSpeed",16,200,true,function(value) --MinValue,MaxValue,Precise
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)

f:Slider("JumpPower",16,200,true,function(value) --MinValue,MaxValue,Precise
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
end)

f:DestroyGUI()
